<tr class="text-center">
		<td class="align-middle"><%= test.title %></td>
		<td class="align-middle"><%= test_level(test) %></td>
		<td class="align-middle"><%= test.questions.count %></td>
		<td ><%= link_to t('.show'), admin_test_path(test), class: "btn btn-primary" %> </td>
		<td><%= link_to t('.edit') , edit_admin_test_path(test), class: "btn btn-primary" %> </td>
		<td><%= button_to t('.delete') ,
            			 admin_test_path(test),
            			  class: "btn btn-danger",
            			 method: :delete %>
 		</td>
</tr>